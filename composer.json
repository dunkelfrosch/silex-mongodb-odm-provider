{
    "name": "df/silex-doctrine-mongodb-odm-provider",
    "description": "Silex Doctrine MongoDB ODM Provider for PHP 7.n, based on saxulum's MongoDb ODM Provider",
    "keywords": ["dunkelfrosch", "df", "pimple", "silex", "doctrine", "mongo", "mongodb", "odm", "database", "service", "provider"],
    "license": "MIT",
    "type": "library",
    "time": "2016-08-01",
    "authors": [
        {
            "name": "Patrick Paechnatz",
            "email": "patrick.paechnatz@gmail.com"
        },
        {
            "name": "Dominik Zogg",
            "email": "dominik.zogg@gmail.com"
        }
    ],
    "support": {
        "email": "patrick.paechnatz@gmail.com",
        "issues": "https://github.com/dunkelfrosch/silex-mongodb-odm-provider/issues",
        "source": "https://github.com/dunkelfrosch/silex-mongodb-odm-provider"
    },
    "provide": {
        "ext-mongo": "1.6.13"
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "psr/log": "1.0.*",
        "doctrine/mongodb": "^1.3.0",
        "mongodb/mongodb": "^1.0.2",
        "doctrine/mongodb-odm": "^1.0.7",
        "df/silex-doctrine-mongodb-provider": "^1.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "silex/silex": "~2.0",
        "pimple/pimple": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "DF\\DoctrineMongoDbOdm\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "process-timeout": 120,
        "use-include-path": false,
        "preferred-install": "auto",
        "discard-changes": true,
        "secure-http": false,
        "minimum-stability": "dev",
        "prefer-stable": true
    },
    "extra": {
        "branch-alias": {
          "dev-master": "1.0-dev"
        }
    }
}
