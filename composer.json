{
    "name": "df/silex-doctrine-mongodb-odm-provider",
    "description": "Dunkelfrosch fork of saxulum doctrine mongoDb ODM provider for usage within php7",
    "keywords": ["dunkelfrosch", "df", "pimple", "silex", "doctrine", "mongo", "mongodb", "odm"],
    "license": "MIT",
    "type": "library",
    "time": "2016-08-01",
    "authors": [
        {
            "name": "Patrick Paechnatz",
            "email": "patrick.paechnatz@gmail.com"
        },
        {
            "name": "Dominik Zogg",
            "email": "dominik.zogg@gmail.com"
        }
    ],
    "replace": {
       "ext-mongo": "1.6.12"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "df/silex-doctrine-mongodb-provider",
                "version": "0.9.9",
                "dist": {
                    "url": "https://github.com/dunkelfrosch/silex-mongodb-provider/archive/0.9.9.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "php": "~5.6|~7.0",
        "psr/log": "1.0.*",
        "alcaeus/mongo-php-adapter": "^1.0.0",
        "doctrine/mongodb": "^1.3.0",
        "doctrine/mongodb-odm": "^1.0.7",
        "mongodb/mongodb": "@beta",
        "df/silex-doctrine-mongodb-provider": "^0.9.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "silex/silex": "2.0.x-dev",
        "pimple/pimple": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "DF\\DoctrineMongoDbOdm\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "process-timeout": 120,
        "use-include-path": false,
        "preferred-install": "auto",
        "discard-changes": true,
        "secure-http": false,
        "minimum-stability": "dev"
    }
}
